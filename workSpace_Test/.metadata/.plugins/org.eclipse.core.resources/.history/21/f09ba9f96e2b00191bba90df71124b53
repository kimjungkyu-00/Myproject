package snail4;

public class main {

	public static void main(String[] args) {
		int arrSize=5;
		int[][] arr=new int[arrSize][arrSize];
		int r=5;
		int c=0;
		int k=arrSize;
		int sw=-1;
		int n=1;
		while(true) {
		for(int i=0;i<k;i++) {
			r+=sw;
			arr[r][c]=n++;
		}
		k--;
		
			if(k==0) {
				break;
			}
			
		}
		
		
	}

}
