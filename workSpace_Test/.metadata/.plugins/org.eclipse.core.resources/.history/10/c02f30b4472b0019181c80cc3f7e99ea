package test8;

public class main {

	public static void main(String[] args) {
		int[][] num=new int[5][5];
		int x=1;
		int check=1;
		for(int i=0; i<num.length;i++) {
			for(int j=0;j<num.length;j++) {
				if(i == 0) {
					num[i][j] = x++;
				}
				
				else if(j ==4) {
					num[i][j]= x++;
				}
				
				else if(i == 4) {
					num[i][j]= check*-1;
					num[i][j]= x++;
				}
//				if(j == 0) {
//					num[i][j]=x++;
//				}
				System.out.print(num[i][j]+ " ");
				
			}System.out.println();
			
			
		}
	}
}