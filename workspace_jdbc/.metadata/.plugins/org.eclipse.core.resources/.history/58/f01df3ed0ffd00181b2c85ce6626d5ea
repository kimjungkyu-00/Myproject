
public class Pet {
	String name;
	String owner;
	String species;
	Date birth;
	Date death;
	
	public Pet(String name, String owner,String species ,Date birth) {
		this.name=name;
		this.owner=owner;
		this.species=species; 
		this.birth=birth;
	}
	
	public Pet() {}
	


	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getOwner() {
		return owner;
	}
	public void setOwner(String owner) {
		this.owner = owner;
	}
	public String getSpecies() {
		return species;
	}
	public void setSpecies(String species) {
		this.species = species;
	}

	
}
