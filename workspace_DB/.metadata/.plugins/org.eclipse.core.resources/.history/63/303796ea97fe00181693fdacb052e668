<%

	MyDB mydb = null;
	
	mydb = new MyDB();
	ArrayList<Member> list = mydb.getMemberList();
	
	if(null != list){
	for(int i=0; i< list.size();++i){
		
		Member mem= list.get(i);
		out.print("<tr><td>"+mem.getId()+"<td>");
		out.print("<td>"+mem.getName()+"<td>");
		out.print("<td>"+mem.getDate()+"<td></tr>");
	}
